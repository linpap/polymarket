{
  "name": "polymarket-agent",
  "version": "1.0.0",
  "description": "Autonomous Polymarket trading agent",
  "main": "dist/src/agent.js",
  "scripts": {
    "build": "tsc",
    "start": "node dist/src/agent.js",
    "dry-run": "DRY_RUN=true node dist/scripts/dry-run.js",
    "setup": "node dist/scripts/setup-wallet.js",
    "balance": "node dist/scripts/check-balance.js",
    "paper-trade": "node dist/scripts/paper-trade.js",
    "report": "node dist/scripts/report.js",
    "backtest": "node dist/scripts/backtest.js",
    "updown": "npx tsx src/updown/index.ts",
    "kalshi": "npx tsx src/kalshi/index.ts"
  },
  "dependencies": {
    "@polymarket/clob-client": "^5.2.0",
    "dotenv": "^16.4.0",
    "ethers": "^5.7.2",
    "ws": "^8.18.0"
  },
  "devDependencies": {
    "@types/node": "^22.0.0",
    "@types/ws": "^8.5.0",
    "typescript": "^5.7.0"
  }
}
